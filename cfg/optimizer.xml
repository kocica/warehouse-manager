               numberDimensions="15"
               problemMin="0"
               problemMax="60"
               populationSize="2000"
               selectionSize="1999"
               eliteSize="1"
               maxGenerations="1000"
               probCrossover="0.6"
               probMutationInd="0.4"
               probMutationGene="0.2"
               saveWeightsAfter="1"
               maxTrialValue="10"
               mutationFunctor="mutateInverse"
               selectionFunctor="selectTournam"
               crossoverFunctor="crossoverOrdered" />
