<!--
        ===================================== General params =====================================

        numberDimensions ................. Number of dimensions (i.e. number of SKUs)
        problemMin ....................... Slot min number
        problemMax ....................... Slot max number
        maxIterations .................... Maximal number of generations
        initialWeights ................... Allocations to start evolution from
        saveWeightsPeriod ................ Save weights after N iterations
        maxTrialValue .................... When exceeded, individual is replaced with random one

        ===================================== Genetic algorithm =====================================

        populationSize ................... Population size
        selectionSize .................... Size of selected part of population
        eliteSize ........................ Size of elite part of population (unchanged)
        probCrossover .................... Probability of crossover
        probMutationInd .................. Probability of individual mutation
        probMutationGene ................. Probability of gene mutation
        mutationFunctor .................. Selected mutation operator
        selectionFunctor ................. Selected selection operator
        crossoverFunctor ................. Selected crossover operator

        ===================================== Artificial bee colony =================================

        foodSize ......................... Number of food locations for honey bee's
        keepBest ......................... Don't drop the best solution found

        =============================================================================================
  -->

<configuration numberDimensions="4"
               problemMin="0"
               problemMax="8"
               maxIterations="1000"
               initialWeights=""
               saveWeightsPeriod="1"
               maxTrialValue="10"

               populationSize="10"
               selectionSize="9"
               eliteSize="1"
               probCrossover="0.6"
               probMutationInd="0.4"
               probMutationGene="0.2"
               mutationFunctor="mutateInverse"
               selectionFunctor="selectTournam"
               crossoverFunctor="crossoverOrdered"

               foodSize="100"
               keepBest="true" />
